{
  "name": "react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:ui": "NODE_ENV=development vite --config frontend/vite.config.js",
    "build:ui": "vite build --config frontend/vite.config.js",
    "prod:ui": "vite preview --config frontend/vite.config.js",
    "test:ui": "NODE_ENV=test vitest run --config frontend/vite.config.js",
    "dev:server": "NODE_ENV=development nodemon backend/index.js",
    "prod:server": "node backend/index.js",
    "test:server": "NODE_ENV=test node --experimental-vm-modules node_modules/.bin/jest --runInBand --rootDir backend",
    "start:test:db": "docker compose -f docker-compose.yml up",
    "lint": "eslint frontend --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@apollo/client": "^3.13.1",
    "@apollo/server": "^4.11.3",
    "@fontsource/roboto": "^5.1.1",
    "@fontsource/roboto-condensed": "^5.1.1",
    "@tailwindcss/vite": "^4.0.6",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "graphql": "^16.10.0",
    "motion": "^12.4.3",
    "pg": "^8.13.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "react-router": "^7.1.5",
    "sequelize": "^6.37.5",
    "sequelize-cursor-pagination": "^3.5.0",
    "tailwindcss": "^4.0.6",
    "umzug": "^3.8.2",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@eslint/js": "^9.19.0",
    "@playwright/test": "^1.50.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-jest": "^29.7.0",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "jsdom": "^26.0.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.7"
  }
}
